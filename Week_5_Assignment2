# Base class: Vehicle
class Vehicle:
    def move(self):
        pass  # Placeholder for subclasses

# Subclass: Car
class Car(Vehicle):
    def move(self):
        return "Driving on the road ğŸš—"

# Subclass: Plane
class Plane(Vehicle):
    def move(self):
        return "Flying in the sky âœˆï¸"

# Subclass: Boat
class Boat(Vehicle):
    def move(self):
        return "Sailing on the water ğŸš¢"

# Using Polymorphism
vehicles = [Car(), Plane(), Boat()]

for vehicle in vehicles:
    print(vehicle.move())
